            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="A surveiller"/>
                <para PARENT="Headers bleu"/>
                <ITEXT CH="Très lourdes pertes russes dans plusieurs unités, la capacité offensive de l'armée russe doit diminuer rapidement"/>
                <para PARENT="Important"/>
                <ITEXT CH="Affaiblissement potentiel russe dans le sud de l'Ukraine, j'ai l'impression que les réserves diminuent."/>
                <para PARENT="Important"/>
                <ITEXT CH="L'artillerie et l'aviation russe continuent à empêcher les opérations ukrainiennes d'importance"/>
                <para PARENT="Important"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="100" YPOS="2682.53" OwnPage="3" ItemID="182403312" PTYPE="7" WIDTH="594.25" HEIGHT="55" FRTYPE="3" CLIPEDIT="1" PWIDTH="1" PCOLOR="Dark blue" PCOLOR2="Black" PLINEART="1" TransValue="0.1" path="M0 55 L594.25 35.3086 L594.25 0 L0 0 " copath="M0 55 L594.25 35.3086 L594.25 0 L0 0 L0 55 Z" gXpos="0" gYpos="0" gWidth="0" gHeight="0" LAYER="0"/>
