	measure 'Assauts russes (disp)' =
			
			VAR __base = [Assauts russes]
			Var __diff = [Assauts russes]-[Assauts russes S-1]
			VAR __signe = if(__diff>0,"+ "&__diff,if(__diff<0,"- "&__diff,""))
			RETURN __base& " ("&__signe&")"
