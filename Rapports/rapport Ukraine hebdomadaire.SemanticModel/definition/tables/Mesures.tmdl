	measure 'Assauts russes S-1' =
			
			VAR _num_semaine = SELECTEDVALUE(Calendrier[Numéro semaine])-1
			RETURN CALCULATE([Assauts russes],Calendrier[Numéro semaine]=_num_semaine)
		formatString: 0
